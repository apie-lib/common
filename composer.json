{
  "name": "apie/common",
  "description": "Composer package of the apie library: common",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Common\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\Common\\Enums\\UrlPrefix",
      "Apie\\Common\\ValueObjects\\EntityNamespace",
      "Apie\\Common\\ValueObjects\\PossibleRoutePrefixes"
    ]
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "apie/serializer": "self.version",
    "defuse/php-encryption": "^2.4",
    "httpsoft/http-cookie": "^1.1.0",
    "psr/http-message": "^1.1",
    "symfony/cache": "^7.2",
    "symfony/config": "^7.2",
    "symfony/finder": "^7.2"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "apie/text-value-objects": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2",
    "phpspec/prophecy-phpunit": "^2.2",
    "symfony/event-dispatcher": "^7.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}