{
  "name": "apie/common",
  "description": "Composer package of the apie library: common",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Common\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/serializer": "self.version",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "apie/composite-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "apie/text-value-objects": "self.version",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}