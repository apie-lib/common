{
  "name": "apie/common",
  "description": "Composer package of the apie library: common",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Common\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\Common\\Enums\\UrlPrefix",
      "Apie\\Common\\ValueObjects\\EntityNamespace",
      "Apie\\Common\\ValueObjects\\PossibleRoutePrefixes"
    ]
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/serializer": "self.version",
    "defuse/php-encryption": "2.*",
    "httpsoft/http-cookie": "^1.1.0",
    "psr/http-message": "^1.0",
    "symfony/cache": "^6.0",
    "symfony/config": "6.*",
    "symfony/finder": "6.*"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/text-value-objects": "self.version",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}